<Window x:Class="Image_Manipulation_App.TwoParamsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Image_Manipulation_App"
        mc:Ignorable="d"
        Title="" Height="150" Width="300" HorizontalAlignment="Center" VerticalAlignment="Center">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBlock x:Name="Param1TextBoxLabel" Text="" />
        <TextBox x:Name="Param1TextBox" TextChanged="ParamTextBox_TextChanged"/>
        <TextBlock x:Name="Param2TextBoxLabel" Text="" />
        <TextBox x:Name="Param2TextBox" TextChanged="ParamTextBox_TextChanged"/>

        <Button Content="" x:Name="ParamsButton" Width="100" Click="OnParamsButtonClicked" IsEnabled="False" Margin="10"/>
    </StackPanel>
</Window>
