<Window x:Class="Image_Manipulation_App.OneParamWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Image_Manipulation_App"
        mc:Ignorable="d"
        Title="" Height="120" Width="300" HorizontalAlignment="Center" VerticalAlignment="Center">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBlock x:Name="ParamTextBoxLabel" Text="" />
        <TextBox x:Name="ParamTextBox" TextChanged="ParamTextBox_TextChanged"/>

        <Button Content="" x:Name="ParamButton" Width="100" Click="OnParamButtonClicked" IsEnabled="False" Margin="10"/>
    </StackPanel>
</Window>
